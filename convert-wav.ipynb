   "source": [
    "# Converts wav files 16 bit\n",
    "\n",
    "import soundfile as sf\n",
    "\n",
    "def wav_to_16bit(name):\n",
    "    data, samplerate = sf.read('sounds/'+name+'.wav')\n",
    "    sf.write('sounds/'+name+'.wav', data, samplerate, subtype='PCM_16')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "wav_to_16bit('piano-a-old')\n",
    "wav_to_16bit('xylophone-a-old')"
   ]
  }
